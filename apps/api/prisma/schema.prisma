generator default {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  uuid       String   @unique @db.Char(191)
  name       String   @db.Char(191)
  password   String?  @db.Char(191)
  salt       String?  @db.Char(32)
  email      String?  @unique @db.Char(191)
  deleted    Boolean  @default(false)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  created_by String   @db.Char(191)
  updated_by String   @db.Char(191)

  @@map("user")
}

model ChatSession {
  id         Int      @id @default(autoincrement())
  uuid       String   @unique @db.Char(191)
  deleted    Boolean  @default(false)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  created_by String   @db.Char(191)
  updated_by String   @db.Char(191)

  @@map("chat_session")
}

model ChatMessage {
  id           Int      @id @default(autoincrement())
  uuid         String   @unique @db.Char(191)
  session_uuid String   @db.Char(191)
  role         String   @db.Char(32)
  status       String   @db.Char(32)
  meta         Json?
  deleted      Boolean  @default(false)
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt
  created_by   String   @db.Char(191)
  updated_by   String   @db.Char(191)

  @@index([session_uuid])
  @@index([created_by, session_uuid])
  @@map("chat_message")
}

model ChatMessageBlock {
  id           Int      @id @default(autoincrement())
  uuid         String   @unique @db.Char(191)
  session_uuid String   @db.Char(191)
  message_uuid String   @db.Char(191)
  content      Json     @db.Json
  deleted      Boolean  @default(false)
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt
  created_by   String   @db.Char(191)
  updated_by   String   @db.Char(191)

  @@index([session_uuid])
  @@index([message_uuid])
  @@index([created_by, session_uuid, message_uuid])
  @@map("chat_message_block")
}

model Task {
  id         Int      @id @default(autoincrement())
  uuid       String   @unique @db.Char(191)
  title      String   @db.Char(32)
  meta       Json     @db.Json
  payload    Json     @db.Json
  status     String   @db.Char(32)
  deleted    Boolean  @default(false)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  created_by String   @db.Char(191)
  updated_by String   @db.Char(191)

  @@index([created_by])
  @@map("task")
}

model TaskResourceRelation {
  id            Int      @id @default(autoincrement())
  uuid          String   @unique @db.Char(191)
  task_uuid     String   @db.Char(191)
  resource_type String   @db.Char(32)
  resource_uuid String   @db.Char(191)
  deleted       Boolean  @default(false)
  created_at    DateTime @default(now())
  updated_at    DateTime @updatedAt
  created_by    String   @db.Char(191)
  updated_by    String   @db.Char(191)

  @@index([task_uuid])
  @@index([resource_uuid])
  @@map("task_resource_relation")
}

model Image {
  id           Int      @id @default(autoincrement())
  uuid         String   @unique @db.Char(191)
  url          String?   @db.Char(2048)
  thumbnail    String?   @db.Char(2048)
  format       String?   @db.Char(32)
  width        Int?
  height       Int?
  size         Int?
  aspect_ratio String?   @db.Char(32)
  resolution   String?   @db.Char(32)
  deleted      Boolean  @default(false)
  created_at   DateTime @default(now())
  updated_at   DateTime @updatedAt
  created_by   String   @db.Char(191)
  updated_by   String   @db.Char(191)

  @@map("image")
}

model SubscriptionQuota {
  id                Int      @id @default(autoincrement())
  uuid              String   @unique @default(uuid()) @db.Char(191)
  balance           Decimal  @default(0) @db.Decimal(18, 4)
  locked_balance    Decimal  @default(0) @db.Decimal(18, 4)
  total_spent       Decimal  @default(0) @db.Decimal(18, 4)
  warning_threshold Decimal  @default(0) @db.Decimal(18, 4)
  deleted           Boolean  @default(false)
  created_at        DateTime @default(now())
  updated_at        DateTime @updatedAt
  created_by        String   @unique @db.Char(191)
  updated_by        String   @db.Char(191)

  @@index([created_by])
  @@map("subscription_quota")
}

model SubscriptionTransaction {
  id                 Int      @id @default(autoincrement())
  uuid               String   @unique @default(uuid()) @db.Char(191)
  external_id        String?  @unique @db.Char(191)
  parent_uuid        String?  @db.Char(191)
  transaction_type   String   @db.Char(32)
  transaction_status String   @db.Char(32)
  change_amount      Decimal  @db.Decimal(18, 4)
  balance_snapshot   Decimal  @db.Decimal(18, 4)
  remark             String?  @db.Char(191)
  deleted            Boolean  @default(false)
  created_at         DateTime @default(now())
  updated_at         DateTime @updatedAt
  created_by         String   @db.Char(191)
  updated_by         String   @db.Char(191)

  @@index([created_by])
  @@index([external_id])
  @@map("subscription_transaction")
}
