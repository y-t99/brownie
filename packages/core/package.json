{
  "name": "@brownie/core",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint \"src/**/*.ts*\"",
    "format": "eslint --fix \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@brownie/eslint-config": "workspace:*",
    "@brownie/typescript-config": "workspace:*",
    "@brownie/toolkit": "workspace:*"
  },
  "dependencies": {
    "xstate": "^5.19.4",
    "@ai-sdk/openai-compatible": "^1.0.11"
  }
}
